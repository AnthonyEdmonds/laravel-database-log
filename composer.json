{
    "name": "anthonyedmonds/laravel-database-log",
    "description": "Store your Laravel logs in the database",
    "keywords": ["laravel", "monolog", "database", "logging"],
    "type": "library",
    "homepage": "https://github.com/AnthonyEdmonds/laravel-database-log",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Edmonds",
            "email": "contact@anthonyedmonds.me",
            "homepage": "https://anthonyedmonds.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8",
        "illuminate/support": "^12"
    },
    "require-dev": {
        "larastan/larastan": "^3",
        "laravel/pint": "^1",
        "orchestra/testbench": "^10",
        "phpunit/phpunit": "^12"
    },
    "autoload": {
        "psr-4": {
            "AnthonyEdmonds\\LaravelDatabaseLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnthonyEdmonds\\LaravelDatabaseLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnthonyEdmonds\\LaravelDatabaseLog\\DatabaseLogServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
